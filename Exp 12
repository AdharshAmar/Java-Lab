import java.util.Random;

public class ExpNo12 {
    public static void main(String[] args) {
        Random random = new Random();
        for (int i = 0; i < 5; i++) {
            int randomNumber = random.nextInt(100) + 1;
            System.out.println("Random number " + (i + 1) + ": " + randomNumber);
        }
    }
}
